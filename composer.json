{
    "name": "nepada/email-address-doctrine",
    "type": "library",
    "license": "BSD-3-Clause",
    "description": "Email address type for Doctrine.",
    "keywords": ["email address", "value object", "doctrine", "nette"],
    "support": {
        "email": "petr@pada.cz",
        "issues": "https://github.com/nepada/email-address-doctrine/issues"
    },
    "authors": [
        {
            "name": "Petr MorÃ¡vek",
            "email": "petr@pada.cz"
        }
    ],
    "require": {
        "php": ">=7.4.0",
        "doctrine/dbal": "^2.6@dev",
        "nepada/email-address": "^2.3@dev || ^3.0@dev"
    },
    "require-dev": {
        "nette/tester": "^2.3.4",
        "mockery/mockery": "^1.4.2",
        "phpstan/phpstan": "^0.12.60",
        "phpstan/phpstan-strict-rules": "^0.12.6",
        "pepakriz/phpstan-exception-rules": "^0.11.2",
        "phpstan/phpstan-nette": "^0.12.11",
        "nepada/phpstan-nette-tester": "^0.3.1",
        "spaze/phpstan-disallowed-calls": "^1.1",
        "phpstan/phpstan-mockery": "^0.12.10",
        "php-parallel-lint/php-parallel-lint": "^1.2.0",
        "nepada/coding-standard": "^7.3"
    },
    "autoload": {
        "psr-4": {
            "Nepada\\EmailAddressDoctrine\\": "src/EmailAddressDoctrine/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NepadaTests\\": "tests/"
        }
    },
    "config": {
        "platform-check": false
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.4-dev"
        }
    }
}
