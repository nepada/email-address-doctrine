{
    "name": "nepada/email-address-doctrine",
    "type": "library",
    "license": "BSD-3-Clause",
    "description": "Email address type for Doctrine.",
    "keywords": ["email address", "value object", "doctrine", "nette"],
    "support": {
        "email": "petr@pada.cz",
        "issues": "https://github.com/nepada/email-address-doctrine/issues"
    },
    "authors": [
        {
            "name": "Petr MorÃ¡vek",
            "email": "petr@pada.cz"
        }
    ],
    "require": {
        "php": ">=8.1.0 <8.5",
        "doctrine/dbal": "^3.0@dev || ^4.0@dev",
        "nepada/email-address": "^2.3@dev || ^3.0@dev"
    },
    "require-dev": {
        "nette/tester": "2.5.6",
        "mockery/mockery": "1.6.12",
        "phpstan/phpstan": "2.1.29",
        "phpstan/phpstan-strict-rules": "2.0.7",
        "phpstan/phpstan-nette": "2.0.6",
        "nepada/phpstan-nette-tester": "2.0.1",
        "spaze/phpstan-disallowed-calls": "4.6.0",
        "shipmonk/phpstan-rules": "4.2.1",
        "phpstan/phpstan-mockery": "2.0.0",
        "php-parallel-lint/php-parallel-lint": "1.4.0",
        "nepada/coding-standard": "8.0.1",
        "composer-runtime-api": "^2.0",
        "composer/semver": "3.4.4",
        "nette/utils": ">=3.2.7"
    },
    "autoload": {
        "psr-4": {
            "Nepada\\EmailAddressDoctrine\\": "src/EmailAddressDoctrine/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NepadaTests\\": "tests/"
        }
    },
    "config": {
        "platform-check": false,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": false
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.8-dev"
        }
    }
}
